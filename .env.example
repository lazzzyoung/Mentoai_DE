# =======================================
# Core service credentials / secrets
# =======================================
GOOGLE_API_KEY=your_gemini_key

# Airflow security and admin bootstrap
AIRFLOW__WEBSERVER__SECRET_KEY=replace_with_secure_random_string
AIRFLOW_ADMIN_USERNAME=admin
AIRFLOW_ADMIN_PASSWORD=replace_with_strong_password
AIRFLOW_ADMIN_FIRSTNAME=Admin
AIRFLOW_ADMIN_LASTNAME=User
AIRFLOW_ADMIN_EMAIL=admin@example.com

# =======================================
# AWS / S3
# =======================================
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=ap-northeast-2
S3_BUCKET_NAME=your_bucket_name

# =======================================
# Kafka topics and scraping
# =======================================
KAFKA_BOOTSTRAP_SERVERS=kafka:29092
KAFKA_TOPIC_NAME=career_raw
# Optional: if unset, backfill producer falls back to KAFKA_TOPIC_NAME
KAFKA_BACKFILL_TOPIC=

WANTED_BASE_URL=https://www.wanted.co.kr
TARGET_JOB_GROUP=518
TARGET_JOB_ID=655
WANTED_LIMIT=50
RECRUIT24_DAILY_LIMIT=50

# =======================================
# Database / vector store
# =======================================
DB_URL=jdbc:postgresql://postgres:5432/mentoai
DB_USER=airflow
DB_PASSWORD=airflow
# Server accepts both postgresql:// and postgresql+asyncpg://
DATABASE_URL=postgresql://airflow:airflow@postgres:5432/mentoai

QDRANT_HOST=mentoai-qdrant
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=career_jobs

# =======================================
# Optional image tag overrides (docker-compose)
# =======================================
ZOOKEEPER_IMAGE_TAG=3.9.3
KAFKA_IMAGE_TAG=3.8.1
KAFKA_UI_IMAGE_TAG=v0.7.2
QDRANT_IMAGE_TAG=v1.13.6
